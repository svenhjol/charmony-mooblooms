{
  "schemaVersion": 1,
  "id": "charmony-mooblooms",
  "version": "${mod_version}",

  "name": "Charmony Mooblooms",
  "description": "Mooblooms are cow-like mobs that come in a variety of flower types. They spawn flowers where they walk and can be milked for suspicious stew.",

  "authors": [
    "svenhjol",
    "Coranthes"
  ],
  "icon": "assets/charmony-mooblooms/textures/gui/sprites/icon.png",
  "license": "MIT",
  "environment": "*",
  "accessWidener": "charmony-mooblooms.accesswidener",
  "entrypoints": {
    "main": [
        "svenhjol.charmony.mooblooms.common.CommonInitializer"
    ],
    "client": [
        "svenhjol.charmony.mooblooms.client.ClientInitializer"
    ],
    "modmenu": [
        "svenhjol.charmony.mooblooms.integration.ModMenuPlugin"
    ]
  },

  "mixins": [],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "*",
    "java": ">=${java_version}",
    "fabric-api": "*",
    "charmony": ">=${charmony_version}",
    "charmony-api": ">=${api_version}"
  },

  "contact": {
    "homepage": "${homepage_link}",
    "sources": "https://github.com/svenhjol/${mod_id}"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord_link}"
      },
      "update_checker": true
    }
  }
}